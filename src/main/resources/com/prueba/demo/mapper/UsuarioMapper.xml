<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.prueba.demo.mapper.UsuarioMapper">

	<resultMap id="resUsuario" type="Usuario">
		<result property="idUsuario" 	    	column="N_USUARIO_PK" />
		<result property="idSucursal" 	   	 	column="N_SUCURSAL" />
        <result property="idPerfil"  	    	column="N_PERFIL" />
        <result property="usuario"          	column="C_USUARIO" />
        <result property="nombres"          	column="C_NOMBRES" />
        <result property="apellidoPaterno"  	column="C_APELLIDO_PATERNO" />
        <result property="apellidoMaterno"  	column="C_APELLIDO_MATERNO" />
        <result property="activo"  	        	column="N_ACTIVO" />
		<result property="usuarioCreacion"  	column="C_USUARIO_CREACION" />
		<result property="fechaCreacion"        column="D_FECHA_CREACION" />
		
  
	</resultMap>

	<select id="getListaUsuario" parameterType="com.prueba.demo.core.model.Usuario" statementType="CALLABLE"
			resultMap="resUsuario">
		{CALL sp_usuario_lis(
			#{usuario, jdbcType = VARCHAR, mode = IN},
			#{password, jdbcType = VARCHAR, mode = IN}
		)
		}
	</select>

	 
</mapper>